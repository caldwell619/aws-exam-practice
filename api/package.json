{
	"name": "api",
	"version": "1.0.0",
	"description": "API for aws-exam-help",
	"scripts": {
		"install-helper": "sh scripts/build/update-lib-version.sh",
		"lint": "eslint \"src/**/*.js\" --ignore-pattern node_modules/",
		"test:unit": "jest tests/unit",
		"test:integration": "jest tests/integration",
		"dev": "sh scripts/build/start-local-api.sh",
		"deploy:dev": "source .env.dev && sh scripts/build/deploy.sh"
	},
	"keywords": [
		"aws",
		"lambda",
		"api",
		"serverless"
	],
	"author": "caldwell619",
	"license": "ISC",
	"devDependencies": {
		"babel-eslint": "^10.0.2",
		"babel-jest": "^24.8.0",
		"eslint": "^6.1.0",
		"jest": "^24.8.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"eslint:recommended"
		],
		"rules": {},
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": 10
		}
	}
}
